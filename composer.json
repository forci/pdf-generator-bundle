{
    "name": "forci/pdf-generator-bundle",
    "type": "symfony-bundle",
    "description": "A WKHTMLTOPDF based PDF Generator",
    "keywords": ["symfony pdf generator", "html to pdf", "symfony html pdf", "pdf bundle", "wucdbm"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "~4.0|~5.0|~6.0",
        "symfony/process": "~4.0|~5.0|~6.0",
        "symfony/twig-bundle": "~4.0|~5.0|~6.0",
        "twig/twig": "~2.0|~3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "wucdbm/php-cs-fixers": "~0.1"
    },
    "suggest": {
        "h4cc/wkhtmltopdf-i386": "0.12.3"
    },
    "autoload": {
        "psr-4": { "Forci\\Bundle\\PdfGenerator\\": "src" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}